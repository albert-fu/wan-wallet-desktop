{
  "name": "wanwallet",
  "productName": "Wan Wallet",
  "version": "1.0.0-beta.1",
  "description": "Wanchain's new multicoin wallet",
  "main": "./build/main.js",
  "author": {
    "name": "Wanchain Foundation",
    "email": "dev@wanchain.org"
  },
  "scripts": {
    "start": "electron .",
    "dev": "npm run dev:renderer",
    "dev:main": "cross-env NODE_ENV=development electron -r @babel/register ./src/main.dev.js ",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./config/webpack/webpack.config.renderer.babel.js --colors",
    "transpile:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.renderer.prod.babel.js --colors",
    "transpile:main": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.main.prod.babel.js --colors",
    "pack:mac": "run-s copy:mac transpile:renderer transpile:main build:mac",
    "pack:win": "run-s copy:win transpile:renderer transpile:main build:win",
    "pack:linux": "run-s copy transpile:renderer transpile:main build:linux",
    "pack:all": "run-s copy transpile:renderer transpile:main build:all",
    "build:mac": "electron-builder --mac -p never",
    "build:win": "electron-builder --win -p never",
    "build:linux": "electron-builder --linux -p never",
    "build:all": "electron-builder -mwl -p never",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "./build.sh",
    "copy:mac": "./build-mac.sh",
    "copy:win": "build.bat",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wanchain/wan-wallet-desktop.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wanchain/wan-wallet-desktop/issues"
  },
  "homepage": "https://github.com/wanchain/wan-wallet-desktop#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/runtime": "^7.4.0",
    "antd": "^3.15.2",
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "bignumber.js": "^8.1.1",
    "desktop-idle": "^1.1.2",
    "dotenv": "^8.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-updater": "^4.0.6",
    "electron-window-state": "^5.0.3",
    "ethereum-client-binaries": "^1.6.4",
    "fs-extra": "^8.0.1",
    "i18next": "^15.0.8",
    "i18next-node-fs-backend": "^2.1.2",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "postcss-flexbugs-fixes": "^3.2.0",
    "qrcode": "^1.3.3",
    "rc-tween-one": "^2.4.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-i18next": "^10.6.1",
    "react-intl-universal": "^1.16.2",
    "react-router-dom": "^5.0.0",
    "trezor-connect": "7.0.2-electron.4",
    "uuid": "^3.3.2",
    "wanchain-js-sdk": "github:wanchain/wanchain-js-sdk#new_wallet",
    "wanchainjs-tx": "^1.5.0",
    "wanchainjs-util": "^5.2.1",
    "web3": "^1.0.0-beta.55",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-regenerator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-root-import": "^6.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.0",
    "electron-builder": "^20.43.0",
    "electron-notarize": "^0.1.1",
    "electron-packager": "^13.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.8.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
